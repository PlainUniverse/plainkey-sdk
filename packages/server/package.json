{
    "name": "@plainkey/server",
    "version": "0.1.0",
    "description": "PlainKey TypeScript server SDK",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs"
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsdown src/index.ts --format esm --dts --sourcemap --clean",
        "test": "vitest",
        "prepublishOnly": "npm run build"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@plainkey/types": "*",
        "@simplewebauthn/server": "^13.2.1"
    },
    "devDependencies": {
        "tsdown": "^0.15.5"
    }
}
